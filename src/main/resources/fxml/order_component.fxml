<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
    <HBox alignment="TOP_CENTER">
        <children>
            <Label text="Order #" />
            <Label fx:id="labelOrderId" text="-1" />
        </children>
        <VBox.margin>
            <Insets top="12.0" />
        </VBox.margin>
    </HBox>
    <HBox>
        <children>
            <Label text="Name" />
            <TextField fx:id="textFieldName">
                <HBox.margin>
                    <Insets left="12.0" />
                </HBox.margin>
            </TextField>
        </children>
        <VBox.margin>
            <Insets left="12.0" right="12.0" top="12.0" />
        </VBox.margin>
    </HBox>
</fx:root>